{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  extends: [
    "config:base",
    ":disableRateLimiting",
    ":semanticCommitsDisabled"
  ],
  automerge: true,
  platformAutomerge: true,
  platformCommit: true,
  assignAutomerge: true,
  reviewers: ["gabrielittner"],
  rebaseWhen: "conflicted",
  prBodyTemplate: "{{{header}}}{{{table}}}{{{notes}}}{{{changelogs}}}{{{controls}}}{{{footer}}}",
  prBodyColumns: ["Package", "Change", "Age"],
  prFooter: "ðŸ”• Ignore: Close this PR and you won't be reminded about these updates again.",
  ignoreDeps: [
  ],
  packageRules: [
    {
      matchDatasources: ["maven"],
      registryUrls: [
        "https://repo.maven.apache.org/maven2",
        "https://dl.google.com/android/maven2",
        "https://plugins.gradle.org/m2",
      ]
    },
    {
      matchPackageNames: [
        "gradle",
      ],
      prBodyNotes: "[Changelog](https://docs.gradle.org/{{{newVersion}}}/release-notes.html)"
    },
    {
      matchPackagePatterns: [
        "com.freeletics.gradle",
      ],
      groupName: "Freeletics Gradle Plugin"
    },
    {
      matchPackagePatterns: [
        "org.jetbrains.kotlin",
        "com.google.devtools.ksp",
      ],
      excludePackagePatterns: [
        "org.jetbrains.kotlinx",
      ],
      groupName: "Kotlin and KSP"
    },
    {
      matchPackagePatterns: [
        "androidx.compose",
        "org.jetbrains.compose",
      ],
      groupName: "Compose"
    }
  ]
}
